<h3>Discover Movies</h3>
<%= button_to "Find Top Rated Movies", "/user/#{@user.id}/movie", method: :get %>

<%= form_with url: "/user/#{@user.id}/movie", method: :get, local: true do |form| %>
  <%= form.text_field :keyword %>
  <%= form.label :keyword %>
  <%= form.submit "Find Movies"  %>
<% end %>
