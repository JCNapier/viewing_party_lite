<h3>Discover Movies</h3>
<%= button_to "Find Top Rated Movies", user_movie_index_path(@user), method: :get %>

<%= form_with url: user_movie_index_path(@user), method: :get, local: true do |form| %>
  <%= form.text_field :keyword %>
  <%= form.label :keyword %>
  <%= form.submit "Find Movies"  %>
<% end %>
